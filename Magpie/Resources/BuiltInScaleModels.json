  {
    "name": "FSR",
    "effects": [
      {
        "effect": "FSR_EASU",
        "scale": [ -1, -1 ]
      },
      {
        "effect": "FSR_RCAS",
        "sharpness": 0.87
      }
    ]
  },
  {
    "name": "Lanczos",
    "effects": [
      {
        "effect": "Lanczos",
        "scale": [ -1, -1 ]
      },
      {
        "effect": "AdaptiveSharpen",
        "curveHeight": 0.3  
      }
    ]
  },
  // ACNet/Anime4K + FSR Models should only be used when the game resolution is <0.5x of the screen resolution. Since ACNet and Anime4k can only double the resolution. 
  // FSR is used to scale the result up to the screen afterwards. Regular SSimDownscaler tends to look broken when it's used for scaling up instad of down.
  // {
  //   "name": "ACNet + FSR",
  //   "effects": [
  //     {
  //       "effect": "ACNet"
  //     },
  //     {
  //       "effect": "FSR_EASU",
  //       "scale": [ -1, -1 ]
  //     },
  //     {
  //       "effect": "FSR_RCAS",
  //       "sharpness": 0.7
  //     }
  //   ]
  // },
  // {
  //   "name": "ANime4k + FSR",
  //   "effects": [
  //     {
  //       "effect": "Anime4K_Upscale_L"
  //     },
  //     {
  //       "effect": "FSR_EASU",
  //       "scale": [ -1, -1 ]
  //     },
  //     {
  //       "effect": "FSR_RCAS",
  //       "sharpness": 0.7
  //     }
  //   ]
  // },
  // Light and Very Light presets are for low end hardware. Use normal presets if you have a capable rig.
  {
    "name": "Anime4K (Light)",
    "effects": [
      {
        "effect": "Anime4K_Upscale_S"
      },
      {
        "effect": "Bicubic",
        "scale": [ -1, -1 ],
        "paramB": 0.0,
        "paramC": 0.5
      }
    ]
  },
  {
    "name": "Anime4K (Very Light)",
    "effects": [
      {
        "effect": "Anime4K_Upscale_S_Lite"
      },
      {
        "effect": "Bicubic_Lite",
        "scale": [ -1, -1 ],
        "paramB": 0.0,
        "paramC": 0.5
      }
    ]
  },
  {
    "name": "ANime4k Denoise + FSR",
    "effects": [
      {
        "effect": "Anime4K_Upscale_Denoise_L"
      },
      {
        "effect": "FSR_EASU",
        "scale": [ -1, -1 ]
      },
      {
        "effect": "FSR_RCAS",
        "sharpness": 0.7
      }
    ]
  },
  {
    "name": "CRT Hylian",
    "effects": [
      {
        "effect": "CRT_Hyllian",
        "scale": [ -1, -1 ],
      }
    ]
  },
  {
    "name": "CRT Easymode",
    "effects": [
      {
        "effect": "CRT_Easymode",
        "scale": [ -1, -1 ],
      }
    ]
  },
  {
    "name": "CRT Geom",
    "effects": [
      {
        "effect": "CRT_Geom",
        "scale": [ -1, -1 ],
        "interlace": 0,
        "curvature":1,
        "radius": 8,
        "cornerSize": 0.01,
        "lum": 1,
        "CRTGamma": 2.4,
        "monitorGamma": 4.4,
        "scanlineWeight": 0.25
      }
    ]
  }
]
